<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee">
 <servlet>
         <servlet-name>register</servlet-name>
         <servlet-class>com.example.advance2.Register</servlet-class>
 </servlet>

 <servlet-mapping>
         <servlet-name>register</servlet-name>
         <url-pattern>/homepage</url-pattern>
 </servlet-mapping>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.example.advance2.Login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>contactForm</servlet-name>
        <servlet-class>com.example.advance2.ContactForm</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>contactForm</servlet-name>
        <url-pattern>/messageSent</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>grade</servlet-name>
        <servlet-class>com.example.advance2.EvaluateAnswer</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>grade</servlet-name>
        <url-pattern>/grade</url-pattern>
    </servlet-mapping>


</web-app>